{
  "name": "turing-shirt-shop",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "serve": "babel-node ./bin/www",
    "react-web": "cd react-web; npm run dev; cd ..;",
    "react-native": "cd react-native; expo start; cd ..;",
    "dev-native": "concurrently \"npm start\" \"npm run react-web\" \"npm run react-native\"",
    "dev": "concurrently \"npm start\" \"npm run react-web\""
  },
  "dependencies": {
    "concurrently": "^4.1.1",
    "cookie-parser": "~1.4.4",
    "crypto-random-string": "^3.0.1",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.1",
    "express-session": "^1.16.2",
    "express-validator": "^6.1.1",
    "fbgraph": "^1.4.4",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql2": "^1.6.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.4",
    "sequelize": "^5.10.2",
    "stripe": "^7.5.0"
  },
  "devDependencies": {
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "webpack": "^4.37.0"
  }
}
